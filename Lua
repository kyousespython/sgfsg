local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("kyo private hub v1.0",  "DarkTheme")

local UniversalTab = Window:NewTab("Misc")
local UniversalSection1 = UniversalTab:NewSection("Player")

UniversalSection1:NewSlider("Walk Speed", "Changes ur Walk Speed.", 500, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)


UniversalSection1:NewSlider("Jump Power", "Changes ur Jump Power.", 500, 50, function(jp)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = jp
end)

UniversalSection1:NewToggle("Toggle Jump Power", "Toggles Jump Power.", function(state)
    
    if state then
        game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = true
    else
        game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = false
    end
end)


UniversalSection1:NewSlider("Fov", "Changes Fov", 120, 70, function(v)
    workspace.CurrentCamera.FieldOfView = v
    print("FOV is [v] ")
end)


local UniversalTab = Window:NewTab("Main")
local UniversalSection1 = UniversalTab:NewSection("Main")


UniversalSection1:NewToggle("Inf Yield", "Opends Admin commands.", function(state)
    if state then
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    else
        
    end
end)

UniversalSection1:NewButton("Inf Jumps", "Enables Infinite Jumps.", function()
    local InfiniteJumpEnabled = true
game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)
end)

UniversalSection1:NewToggle("Anti Afk", "Never get kicked.", function(state)
    if state then
        loadstring(game:HttpGet("https://pastebin.com/raw/sDXcYFhR", true))()
    else
        
    end
end)

local UniversalTab = Window:NewTab("Autofarms")
local UniversalSection1 = UniversalTab:NewSection("stfu")

UniversalSection1:NewToggle("Fake Hatch", "Auto Hatch nigga.", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/hackerDashDash/ClickerSimulator/main/clickerSimulatorFakeHatcher.lua",true))()
    else
        
    end
end)

UniversalSection1:NewToggle("Autofarm GUI", "Opens autofarm script.", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/scriptpastebin/raw/main/34"))()
    else
        
    end
end)

UniversalSection1:NewToggle("Clicker Hub (not recommended)", "Opens script.", function(state)
    if state then
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    else
        
    end
end)

UniversalSection1:NewToggle("Kyo Private (very recommended)", "Opens kyo script.", function(state)
    if state then
        loadstring(game:HttpGet('https://raw.githubusercontent.com/kyousespython/kyos/main/clicker-simulator'))()
    else
        
    end
end)



local UniversalTab = Window:NewTab("Soon")
local UniversalSection1 = UniversalTab:NewSection("Nigga")








































local Version = "0.0.3"


local HomeTab = Window:NewTab("Credits")
local HomeSection = HomeTab:NewSection("Home")

HomeSection:NewLabel("Welcome, " .. game.Players.LocalPlayer.Name .. "to Kyos Hub")
HomeSection:NewLabel("Version is " .. Version .. ".")

HomeSection:NewLabel("made by kyo#2222")
